{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "globals": {
    "browser": "readonly"
  },
  "rules": {
    "no-plusplus": "off",
    "no-console": "off",
    "default-case": "off",
    "no-continue": "off",
    "prefer-destructuring": "off",
    "no-bitwise": "off",
    "import/prefer-default-export": "off",
    "no-param-reassign": "off"
  }
}
